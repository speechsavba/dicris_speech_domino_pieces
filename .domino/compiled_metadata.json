    "DicrisDatasetPiece": {
        "name": "DicrisDatasetPiece",
        "dependency": {
            "dockerfile": null,
            "requirements_file": "requirements_0.txt"
        },
        "tags": [
            "default",
            "datasets"
        ],
        "style": {
            "node_label": "Dicris Dataset",
            "node_type": "default",
            "node_style": {
                "backgroundColor": "#ebebeb"
            },
            "useIcon": true,
            "icon_class_name": "lucide:toy-brick",
            "iconStyle": {
                "cursor": "pointer"
            }
        },
        "description": "Provide random audio files from DiCris",
        "input_schema": {
            "$defs": {
                "DatasetFile": {
                    "enum": [
                        "http://speech.savba.sk/DiCris/echo_20250716_113400_7B-3.wav",
                        "http://speech.savba.sk/DiCris/echo_20250715_094400_7B-0.wav",
                        "http://speech.savba.sk/DiCris/echo_20250716_104301_D5.wav",
                        "random"
                    ],
                    "title": "DatasetFile",
                    "type": "string"
                }
            },
            "properties": {
                "dataset": {
                    "allOf": [
                        {
                            "$ref": "#/$defs/DatasetFile"
                        }
                    ],
                    "default": "",
                    "title": "Wav name"
                }
            },
            "title": "InputModel",
            "type": "object"
        },
        "output_schema": {
            "properties": {
                "file_path": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "File path"
                }
            },
            "title": "OutputModel",
            "type": "object"
        },
        "secrets_schema": null,
        "source_url": "https://github.com/speechsavba/dicris_speech_domino_pieces/tree/main/pieces/DicrisDatasetPiece"
    },
